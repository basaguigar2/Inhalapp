<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="389.0" prefWidth="602.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="inhalapp.RegisterController">
   <children>
      <ImageView fitHeight="409.0" fitWidth="601.0" layoutX="-1.0" layoutY="-6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/pngtree-minimalist-gradient-medical-background-picture-image_966366.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="username" layoutX="82.0" layoutY="126.0" prefHeight="25.0" prefWidth="182.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="82.0" layoutY="208.0" prefHeight="25.0" prefWidth="182.0" promptText="Password" />
      <PasswordField fx:id="repeatpassword" layoutX="82.0" layoutY="249.0" prefHeight="25.0" prefWidth="182.0" promptText="RepeatPassword" />
      <TextField fx:id="emailtxt" layoutX="82.0" layoutY="167.0" prefHeight="25.0" prefWidth="182.0" promptText="example@gmail.com" />
      <ImageView fitHeight="194.0" fitWidth="207.0" layoutX="330.0" layoutY="111.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/2287797.png" />
         </image>
      </ImageView>
      <Text fill="#0259a4" layoutX="42.0" layoutY="82.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to Inhalapp, let's proceed with the register">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Text>
      <Button fx:id="register" layoutX="82.0" layoutY="289.0" mnemonicParsing="false" onAction="#RegisterUser" prefHeight="18.0" prefWidth="182.0" text="Register" textFill="#03407e">
         <effect>
            <Blend mode="BLUE">
               <topInput>
                  <Bloom threshold="0.0" />
               </topInput>
            </Blend>
         </effect>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Button>
      <Button fx:id="backButton" layoutX="14.0" layoutY="344.0" mnemonicParsing="false" onAction="#backtoMenu" prefHeight="32.0" prefWidth="62.0" text="Back" textFill="RED">
         <effect>
            <Blend mode="RED">
               <topInput>
                  <Bloom />
               </topInput>
            </Blend>
         </effect>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
